procedure calculate(calculate/x : Bv32, calculate/y : Bv32) -> (calculate/RET_VAL : Bv32)
{
    var calculate/RET_VAL : Bv32
    var calculate/sum : Bv32

    loc $0 entry 
    loc $1 final 
    loc $2
    loc $3

    transition $0 -> $2
        assume true
    {
    };

    transition $2 -> $3
        assume true
    {
        calculate/sum := calculate/x + calculate/y;
    };

    transition $3 -> $1
        assume true
    {
        calculate/RET_VAL := calculate/sum;
    };

}
