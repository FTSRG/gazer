main process main_process {
	var locked__test : bool
	procedure Enter_test() {
		init loc L0
		final loc L1
		error loc LE
		L0 -> L1 {
			assume not locked__test
			locked__test := true
		}
		L0 -> LE {
			assume locked__test
		}
	}
	procedure Exit_test() {
		init loc L0
		final loc L1
		error loc LE
		L0 -> L1 {
			assume locked__test
			locked__test := false
		}
		L0 -> LE {
			assume not locked__test
		}
	}
	procedure ctor_locked__test() {
		init loc L0
		final loc L1
		L0 -> L1 {
			locked__test := false
		}
	}
	procedure dtor_locked__test() {
		init loc L0
		final loc L1
		error loc LE
		L0 -> L1 { assume not locked__test }
		L0 -> LE { assume locked__test}
	}
}
